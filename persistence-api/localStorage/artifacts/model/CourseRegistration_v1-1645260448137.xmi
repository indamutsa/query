<?xml version="1.0" encoding="UTF-8"?>
<oopl:Program
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:oopl="http://SimpleOOPL/1.0"
    xsi:schemaLocation="http://SimpleOOPL/1.0 SimpleOOP.ecore"
    pName="CourseRegistration">
  <classes name="Account">
    <attributes
        attrName="accountId"
        attrType="EInt"/>
    <attributes
        attrName="type"
        attrType="EString"/>
    <attributes
        attrName="studentId"
        attrType="EString"/>
  </classes>
  <classes name="Registration">
    <attributes
        attrName="accountId"
        attrType="EInt"/>
    <attributes
        attrName="courseCode"
        attrType="EString"/>
    <attributes
        attrName="examID"
        attrType="EString"/>
    <attributes
        attrName="grade"
        attrType="EString"/>
    <attributes
        attrName="registrationCode"
        attrType="Eint"/>
    <methods
        name="newRegistration"
        returnType="EInt"/>
  </classes>
  <classes name="Student">
    <attributes
        attrName="studentId"
        attrType="EInt"/>
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="major"
        attrType="EString"/>
    <attributes
        attrName="gender"
        attrType="EString"/>
    <attributes
        attrName="dateOfBirth"
        attrType="EDate"/>
    <attributes
        attrName="email"
        attrType="EString"/>
    <attributes
        attrName="address"
        attrType="EString"/>
    <attributes
        attrName="contactNo"
        attrType="EString"/>
    <attributes
        attrName="user"
        attrType="EString"/>
    <attributes
        attrName="password"
        attrType="EString"/>
    <methods
        name="newStudent"
        returnType="EInt"/>
  </classes>
  <classes name="Course">
    <attributes
        attrName="courseCode"
        attrType="EString"/>
    <attributes
        attrName="title"
        attrType="EString"/>
    <attributes
        attrName="type"
        attrType="EString"/>
    <attributes
        attrName="credit"
        attrType="EInt"/>
    <attributes
        attrName="date"
        attrType="EDate"/>
    <attributes
        attrName="time"
        attrType="EString"/>
    <attributes
        attrName="venue"
        attrType="EString"/>
    <attributes
        attrName="instructor"
        attrType="EString"/>
    <methods
        name="newCourse"
        returnType="EString"/>
  </classes>
</oopl:Program>
