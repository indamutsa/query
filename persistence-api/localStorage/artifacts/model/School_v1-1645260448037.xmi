<?xml version="1.0" encoding="UTF-8"?>
<oopl:Program
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:oopl="http://SimpleOOPL/1.0"
    xsi:schemaLocation="http://SimpleOOPL/1.0 SimpleOOP.ecore"
    pName="School">
  <classes name="School">
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="address"
        attrType="EString"/>
  </classes>
  <classes name="Teacher">
    <attributes
        attrName="id"
        attrType="EString"/>
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="date_of_start"
        attrType="EDate"/>
    <attributes
        attrName="last_name"
        attrType="EString"/>
    <methods
        name="addTeacher"
        returnType="EInt"/>
    <methods
        name="deleteTeacher"
        returnType="EInt"/>
    <methods
        name="updateTeacherInformation"
        returnType="EInt"/>
  </classes>
  <classes name="Year">
    <attributes
        attrName="starting_date"
        attrType="EInt"/>
  </classes>
  <classes name="Course">
    <attributes
        attrName="subject"
        attrType="EString"/>
    <attributes
        attrName="weight"
        attrType="EInt"/>
    <attributes
        attrName="credit"
        attrType="EInt"/>
    <methods
        name="newCourse"
        returnType="Eint"/>
    <methods
        name="deleteCourse"
        returnType="Eint"/>
    <methods
        name="editCourse"
        returnType="Eint"/>
    <methods
        name="assignProfessor"
        returnType="Eint"/>
  </classes>
  <classes name="SchoolClass">
    <attributes
        attrName="code"
        attrType="EString"/>
  </classes>
  <classes name="Student">
    <attributes
        attrName="id"
        attrType="EInt"/>
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="lastname"
        attrType="EString"/>
    <attributes
        attrName="day_of_birth"
        attrType="EDate"/>
    <methods
        name="addStudent"
        returnType="EInt"/>
    <methods
        name="deleteStudent"
        returnType="EInt"/>
    <methods
        name="updateStudentInformation"
        returnType="EInt">
      <parameterList
          name="id"
          type="EInt"/>
    </methods>
  </classes>
</oopl:Program>
