<?xml version="1.0" encoding="UTF-8"?>
<oopl:Program
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:oopl="http://SimpleOOPL/1.0"
    xsi:schemaLocation="http://SimpleOOPL/1.0 SimpleOOP.ecore"
    pName="CarRental">
  <classes name="User">
    <attributes
        attrName="userID"
        attrType="EInt"/>
    <attributes
        attrName="userName"
        attrType="EString"/>
    <attributes
        attrName="userAdd"
        attrType="EString"/>
    <attributes
        attrName="userEmail"
        attrType="EString"/>
    <attributes
        attrName="userContactNo"
        attrType="EDouble"/>
    <attributes
        attrName="dob"
        attrType="EDate"/>
    <methods
        name="signUp"
        returnType="EInt"/>
    <methods
        name="login"
        returnType="EString"/>
    <methods
        name="logout"
        returnType="EString"/>
    <methods
        name="newUser"
        returnType="EInt"/>
  </classes>
  <classes name="Vehicle">
    <attributes
        attrName="vehicleType"
        attrType="EString"/>
    <attributes
        attrName="vehicleID"
        attrType="EInt"/>
    <attributes
        attrName="manufacter"
        attrType="EString"/>
    <attributes
        attrName="power"
        attrType="EString"/>
    <attributes
        attrName="regNo"
        attrType="EInt"/>
    <attributes
        attrName="rented"
        attrType="EBoolean"/>
    <attributes
        attrName="color"
        attrType="EString"/>
    <attributes
        attrName="year"
        attrType="EInt"/>
    <methods
        name="rentVehicle"
        returnType="EInt"/>
  </classes>
  <classes name="Car"
      superClasses="//@classes.1">
    <attributes
        attrName="noSeats"
        attrType="EInt"/>
  </classes>
  <classes name="Truck"
      superClasses="//@classes.1">
    <attributes
        attrName="noSeats"
        attrType="EInt"/>
    <attributes
        attrName="weight"
        attrType="EFloat"/>
  </classes>
  <classes name="Motorbike"
      superClasses="//@classes.1">
    <attributes
        attrName="cylinderCap"
        attrType="EInt"/>
  </classes>
  <classes name="Payment">
    <attributes
        attrName="paymentID"
        attrType="EInt"/>
    <attributes
        attrName="paymentType"
        attrType="EString"/>
    <attributes
        attrName="paymentDetails"
        attrType="EString"/>
    <attributes
        attrName="rewards"
        attrType="EString"/>
    <methods
        name="executePayment"
        returnType="EInt"/>
    <methods
        name="registerCreditCard"
        returnType="EInt"/>
  </classes>
  <classes name="Driver">
    <attributes
        attrName="driverID"
        attrType="EInt"/>
    <attributes
        attrName="driverName"
        attrType="EString"/>
    <attributes
        attrName="driverAdd"
        attrType="EString"/>
    <attributes
        attrName="driverEmail"
        attrType="EString"/>
    <attributes
        attrName="driverContactNo"
        attrType="EDouble"/>
    <attributes
        attrName="dob"
        attrType="EDate"/>
    <methods
        name="signUp"
        returnType="EInt"/>
    <methods
        name="login"
        returnType="EBoolean"/>
    <methods
        name="logout"
        returnType="EBoolean"/>
  </classes>
  <classes name="Reservation">
    <attributes
        attrName="from"
        attrType="EDate"/>
    <attributes
        attrName="to"
        attrType="EDate"/>
    <attributes
        attrName="userID"
        attrType="EInt"/>
    <attributes
        attrName="vehicleID"
        attrType="EInt"/>
    <attributes
        attrName="paymentID"
        attrType="EInt"/>
    <attributes
        attrName="driverID"
        attrType="EInt"/>
  </classes>
</oopl:Program>
