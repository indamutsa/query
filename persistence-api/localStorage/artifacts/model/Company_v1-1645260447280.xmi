<?xml version="1.0" encoding="UTF-8"?>
<oopl:Program
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:oopl="http://SimpleOOPL/1.0"
    xsi:schemaLocation="http://SimpleOOPL/1.0 SimpleOOP.ecore"
    pName="Company">
  <classes name="Company">
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="address"
        attrType="EString"/>
    <attributes
        attrName="phoneNumber"
        attrType="EInt"/>
    <attributes
        attrName="email"
        attrType="EString"/>
    <attributes
        attrName="noEmployees"
        attrType="EInt"/>
    <attributes
        attrName="type"
        attrType="EString"/>
  </classes>
  <classes name="Department">
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="noEmployees"
        attrType="EInt"/>
    <attributes
        attrName="type"
        attrType="EString"/>
    <methods
        name="createDepartment"
        returnType="EString"/>
  </classes>
  <classes name="Employee">
    <attributes
        attrName="id"
        attrType="EInt"/>
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="role"
        attrType="EString"/>
    <attributes
        attrName="lastname"
        attrType="EString"/>
    <attributes
        attrName="address"
        attrType="EString"/>
    <attributes
        attrName="phoneNumber"
        attrType="EInt"/>
    <attributes
        attrName="email"
        attrType="EString"/>
    <methods
        name="newEmployee"
        returnType="Eint"/>
  </classes>
  <classes name="Office">
    <attributes
        attrName="address"
        attrType="EString"/>
    <attributes
        attrName="phoneNumber"
        attrType="EInt"/>
    <attributes
        attrName="email"
        attrType="EString"/>
    <attributes
        attrName="noEmployees"
        attrType="EInt"/>
    <attributes
        attrName="type"
        attrType="EString"/>
    <attributes
        attrName="id"
        attrType="EInt"/>
    <methods
        name="createOffice"
        returnType="EInt"/>
  </classes>
  <classes name="Headquarter">
    <attributes
        attrName="address"
        attrType="EString"/>
    <attributes
        attrName="phoneNumber"
        attrType="EInt"/>
    <attributes
        attrName="email"
        attrType="EString"/>
    <attributes
        attrName="noEmployees"
        attrType="EInt"/>
    <attributes
        attrName="type"
        attrType="EString"/>
    <attributes
        attrName="id"
        attrType="EInt"/>
    <methods
        name="addOffice"
        returnType="EInt"/>
  </classes>
</oopl:Program>
