<?xml version="1.0" encoding="UTF-8"?>
<oopl:Program
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:oopl="http://SimpleOOPL/1.0"
    xsi:schemaLocation="http://SimpleOOPL/1.0 SimpleOOP.ecore"
    pName="Zoo">
  <classes name="Animal"
      isAbstract="true">
    <attributes
        attrName="age"
        attrType="EInt"/>
    <attributes
        attrName="gender"
        attrType="EString"/>
    <attributes
        attrName="id"
        attrType="EInt"/>
  </classes>
  <classes name="Oviparous"
      superClasses="//@classes.0">
    <methods
        name="LayEggs"
        returnType="EBoolean"/>
  </classes>
  <classes name="Mammal"
      superClasses="//@classes.0">
    <methods
        name="Nurse"
        returnType="EBoolean"/>
  </classes>
  <classes name="Pelican"
      superClasses="//@classes.1">
    <attributes
        attrName="beakColor"
        attrType="EString"/>
  </classes>
  <classes name="Crocodile"
      superClasses="//@classes.1">
    <attributes
        attrName="weight"
        attrType="EInt"/>
    <attributes
        attrName="height"
        attrType="EInt"/>
  </classes>
  <classes name="SeaLion"
      superClasses="//@classes.2">
    <attributes
        attrName="sizeInFt"
        attrType="EInt"/>
    <attributes
        attrName="female"
        attrType="EBoolean"/>
    <attributes
        attrName="isWild"
        attrType="EBoolean"/>
  </classes>
  <classes name="Whale"
      superClasses="//@classes.2">
    <attributes
        attrName="isWild"
        attrType="EBoolean"/>
  </classes>
  <classes name="Zoo">
    <attributes
        attrName="id"
        attrType="EInt"/>
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="noOfAnimals"
        attrType="EInt"/>
    <attributes
        attrName="capacity"
        attrType="EInt"/>
    <attributes
        attrName="zooManager"
        attrType="EString"/>
    <attributes
        attrName="streetAddress"
        attrType="EString"/>
    <attributes
        attrName="city"
        attrType="EString"/>
    <attributes
        attrName="state"
        attrType="EString"/>
    <attributes
        attrName="zipcode"
        attrType="EInt"/>
    <methods
        name="addAnimal"
        returnType="EInt"/>
    <methods
        name="editAnimal"
        returnType="EInt"/>
  </classes>
  <classes name="Food">
    <attributes
        attrName="id"
        attrType="EInt"/>
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="description"
        attrType="EString"/>
    <attributes
        attrName="vendor"
        attrType="EString"/>
    <attributes
        attrName="units"
        attrType="EInt"/>
  </classes>
</oopl:Program>
