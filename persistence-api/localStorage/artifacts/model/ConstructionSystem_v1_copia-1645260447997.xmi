<?xml version="1.0" encoding="UTF-8"?>
<oopl:Program
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:oopl="http://SimpleOOPL/1.0"
    xsi:schemaLocation="http://SimpleOOPL/1.0 SimpleOOP.ecore"
    pName="ConstructionSystem">
  <classes name="Building"/>
  <classes name="Room">
    <attributes
        attrName="height"
        attrType="EDouble"/>
    <attributes
        attrName="width"
        attrType="EDouble"/>
    <attributes
        attrName="id"
        attrType="EInt"/>
  </classes>
  <classes name="Structure">
    <attributes
        attrName="id"
        attrType="EInt"/>
  </classes>
  <classes name="Window">
    <attributes
        attrName="opacity"
        attrType="EDouble"/>
    <attributes
        attrName="notClosed"
        attrType="EBoolean"/>
    <attributes
        attrName="id"
        attrType="EInt"/>
  </classes>
  <classes name="Fence">
    <attributes
        attrName="inside"
        attrType="EString"/>
    <attributes
        attrName="outside"
        attrType="EString"/>
    <attributes
        attrName="id"
        attrType="EInt"/>
  </classes>
  <classes name="Amenities">
    <attributes
        attrName="id"
        attrType="EInt"/>
    <attributes
        attrName="height"
        attrType="EDouble"/>
    <attributes
        attrName="width"
        attrType="EDouble"/>
  </classes>
</oopl:Program>
