<?xml version="1.0" encoding="UTF-8"?>
<oopl:Program
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:oopl="http://SimpleOOPL/1.0"
    xsi:schemaLocation="http://SimpleOOPL/1.0 SimpleOOP.ecore"
    pName="Application">
  <classes name="Application">
    <attributes
        attrName="name"
        attrType="EString"
        isPublic="true"/>
  </classes>
  <classes name="Component">
    <attributes
        attrName="id"
        attrType="EInt"
        isPublic="true"/>
    <attributes
        attrName="name"
        attrType="EString"
        isPublic="true"/>
  </classes>
  <classes name="Port">
    <attributes
        attrName="id"
        attrType="EInt"
        isPublic="true"/>
    <attributes
        attrName="name"
        attrType="EString"
        isPublic="true"/>
    <attributes
        attrName="capacity"
        attrType="EInt"
        isPublic="true"/>
    <attributes
        attrName="in"
        attrType="EBoolean"/>
  </classes>
  <classes name="Interface">
    <attributes
        attrName="id"
        attrType="EInt"
        isPublic="true"/>
    <attributes
        attrName="name"
        attrType="EString"
        isPublic="false"/>
  </classes>
</oopl:Program>
