<?xml version="1.0" encoding="UTF-8"?>
<oopl:Program
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:oopl="http://SimpleOOPL/1.0"
    xsi:schemaLocation="http://SimpleOOPL/1.0 SimpleOOP.ecore"
    pName="Library">
  <classes name="Author">
    <attributes
        attrName="id"
        attrType="EInt"/>
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="lastname"
        attrType="EString"/>
    <attributes
        attrName="country"
        attrType="EString"/>
  </classes>
  <classes name="Classification">
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="id"
        attrType="EInt"/>
    <methods
        name="addClass"
        returnType="EString"/>
  </classes>
  <classes name="Book">
    <attributes
        attrName="id"
        attrType="EInt"/>
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="amount"
        attrType="EInt"/>
    <attributes
        attrName="edition"
        attrType="EInt"/>
    <attributes
        attrName="category"
        attrType="EString"/>
  </classes>
  <classes name="Building">
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="address"
        attrType="EString"/>
    <attributes
        attrName="telephone"
        attrType="EString"/>
  </classes>
  <classes name="User">
    <attributes
        attrName="membership_category"
        attrType="EString"/>
    <attributes
        attrName="name"
        attrType="EString"/>
    <methods
        name="enroll"
        returnType="EInt"/>
  </classes>
  <classes name="Loan">
    <attributes
        attrName="date"
        attrType="EDate"/>
    <attributes
        attrName="book_code"
        attrType="EInt"/>
    <attributes
        attrName="from"
        attrType="EDate"/>
    <attributes
        attrName="to"
        attrType="EDate"/>
    <attributes
        attrName="status"
        attrType="EString"/>
    <methods
        name="return"
        returnType="EBoolean"/>
  </classes>
</oopl:Program>
