<?xml version="1.0" encoding="UTF-8"?>
<oopl:Program
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:oopl="http://SimpleOOPL/1.0"
    xsi:schemaLocation="http://SimpleOOPL/1.0 SimpleOOP.ecore"
    pName="MovieIMDB">
  <classes name="IMDB_database">
    <attributes
        attrName="name"
        attrType="EString"/>
    <methods
        name="add_movie"
        returnType="EInt"/>
    <methods
        name="add_actor"
        returnType="EInt"/>
    <methods
        name="delete_movie"
        returnType="EInt"/>
    <methods
        name="delete_actor"
        returnType="EInt"/>
    <methods
        name="modify_movie"
        returnType="EInt"/>
    <methods
        name="modify_actor"
        returnType="EInt"/>
  </classes>
  <classes name="Movie_genre_types">
    <attributes
        attrName="movie_id"
        attrType="EInt"/>
    <attributes
        attrName="movie_genre_type"
        attrType="EString"/>
  </classes>
  <classes name="Movie">
    <attributes
        attrName="movie_id"
        attrType="EInt"
        isPublic="true"/>
    <attributes
        attrName="movie_title"
        attrType="EString"
        isPublic="true"/>
    <attributes
        attrName="year_of_production"
        attrType="EInt"/>
    <attributes
        attrName="start_rating"
        attrType="EInt"/>
    <attributes
        attrName="running_time_minutes"
        attrType="EInt"
        isPublic="true"/>
    <attributes
        attrName="trailer"
        attrType="EBoolean"
        isPublic="true"/>
    <attributes
        attrName="synopsys"
        attrType="EString"
        isPublic="true"/>
    <attributes
        attrName="awards"
        attrType="Eint"/>
    <attributes
        attrName="country_of_origin"
        attrType="EString"/>
    <attributes
        attrName="mainDirector"
        attrType="EString"/>
    <attributes
        attrName="awards"
        attrType="Eint"/>
    <attributes
        attrName="budget"
        attrType="EDouble"/>
    <attributes
        attrName="producer"
        attrType="EString"/>
    <attributes
        attrName="admissions"
        attrType="EInt"/>
    <attributes
        attrName="release_date"
        attrType="EDate"/>
    <attributes
        attrName="userCount"
        attrType="EInt"/>
    <attributes
        attrName="abstract"
        attrType="EString"/>
    <attributes
        attrName="trailer"
        attrType="EBoolean"/>
  </classes>
  <classes name="Actor">
    <attributes
        attrName="id"
        attrType="EInt"
        isPublic="true"/>
    <attributes
        attrName="name"
        attrType="EString"
        isPublic="false"/>
    <attributes
        attrName="lastname"
        attrType="EString"
        isPublic="false"/>
    <attributes
        attrName="day_of_birth"
        attrType="EDate"
        isPublic="true"/>
    <attributes
        attrName="address"
        attrType="EString"
        isPublic="false"/>
    <attributes
        attrName="rating"
        attrType="EInt"/>
    <attributes
        attrName="nationality"
        attrType="EString"/>
    <attributes
        attrName="gender"
        attrType="EString"/>
  </classes>
  <classes name="Movie_reviews">
    <attributes
        attrName="movie_id"
        attrType="EInt"
        isPublic="true"/>
    <attributes
        attrName="review_id"
        attrType="EInt"
        isPublic="true"/>
    <attributes
        attrName="start_rating"
        attrType="EInt"/>
    <attributes
        attrName="favorable"
        attrType="EBoolean"/>
    <attributes
        attrName="helpful_count"
        attrType="EInt"/>
    <attributes
        attrName="unhelpful_count"
        attrType="EInt"/>
    <attributes
        attrName="review_text"
        attrType="EString"/>
    <attributes
        attrName="review_date"
        attrType="EDate"/>
  </classes>
</oopl:Program>
