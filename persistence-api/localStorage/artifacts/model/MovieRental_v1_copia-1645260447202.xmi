<?xml version="1.0" encoding="UTF-8"?>
<oopl:Program
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:oopl="http://SimpleOOPL/1.0"
    xsi:schemaLocation="http://SimpleOOPL/1.0 SimpleOOP.ecore"
    pName="Movies">
  <classes name="Movie">
    <attributes
        attrName="id"
        attrType="EInt"/>
    <attributes
        attrName="year"
        attrType="EInt"/>
  </classes>
  <classes name="Rental_Element">
    <attributes
        attrName="fee"
        attrType="EFloat"/>
  </classes>
  <classes name="Rental">
    <attributes
        attrName="number"
        attrType="EInt"/>
    <attributes
        attrName="rented_on"
        attrType="EDate"/>
    <attributes
        attrName="discount"
        attrType="EFloat"/>
  </classes>
  <classes name="Customer">
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="id"
        attrType="EInt"/>
  </classes>
</oopl:Program>
