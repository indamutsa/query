<?xml version="1.0" encoding="UTF-8"?>
<oopl:Program
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:oopl="http://SimpleOOPL/1.0"
    xsi:schemaLocation="http://SimpleOOPL/1.0 SimpleOOP.ecore"
    pName="Airport">
  <classes name="Airport">
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="IATAcode"
        attrType="EString"/>
    <attributes
        attrName="ICAOcode"
        attrType="EString"/>
    <attributes
        attrName="country"
        attrType="EString"/>
    <attributes
        attrName="checkInDesk"
        attrType="EBoolean"/>
    <methods
        name="openCheckInDesk"
        returnType="EBoolean"/>
    <methods
        name="closeCheckInDesk"
        returnType="EBoolean"/>
    <methods
        name="assignCheckInDesk"
        returnType="EBoolean"/>
    <methods
        name="checkInPassenger"
        returnType="EBoolean"/>
  </classes>
  <classes name="Person">
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="surname"
        attrType="EString"/>
    <attributes
        attrName="email"
        attrType="EString"/>
    <attributes
        attrName="gender"
        attrType="EString"/>
    <attributes
        attrName="phoneNumber"
        attrType="EInt"/>
    <attributes
        attrName="dob"
        attrType="EDate"/>
  </classes>
  <classes name="Pilot"
      superClasses="//@classes.1">
    <attributes
        attrName="cathegory"
        attrType="EString"/>
    <attributes
        attrName="id"
        attrType="EInt"/>
  </classes>
  <classes name="Passenger"
      superClasses="//@classes.1">
    <attributes
        attrName="id"
        attrType="EInt"/>
    <attributes
        attrName="regularPassenger"
        attrType="EBoolean"/>
    <attributes
        attrName="hasTicket"
        attrType="EBoolean"/>
  </classes>
  <classes name="Flight">
    <attributes
        attrName="flightCode"
        attrType="EString"/>
    <attributes
        attrName="FlightDate"
        attrType="EDate"/>
    <attributes
        attrName="boardingTime"
        attrType="EDate"/>
    <attributes
        attrName="gate"
        attrType="EString"/>
    <attributes
        attrName="flightSource"
        attrType="EString"/>
    <attributes
        attrName="flightDestination"
        attrType="EString"/>
    <attributes
        attrName="groundSpeed"
        attrType="EFloat"/>
    <attributes
        attrName="altitude"
        attrType="EFloar"/>
    <attributes
        attrName="latitude"
        attrType="EFloat"/>
    <attributes
        attrName="turnRate"
        attrType="EFloat"/>
    <attributes
        attrName="peachRate"
        attrType="EFloat"/>
    <attributes
        attrName="flightRules"
        attrType="EString"/>
    <attributes
        attrName="state"
        attrType="EString"/>
  </classes>
  <classes name="Ticket">
    <attributes
        attrName="number"
        attrType="EInt"/>
    <attributes
        attrName="flightCode"
        attrType="EString"/>
    <attributes
        attrName="FlightDate"
        attrType="EDate"/>
    <attributes
        attrName="boardingTime"
        attrType="EDate"/>
    <attributes
        attrName="gate"
        attrType="EString"/>
    <attributes
        attrName="flightSource"
        attrType="EString"/>
    <attributes
        attrName="flightDestination"
        attrType="EString"/>
  </classes>
  <classes name="Plane">
    <attributes
        attrName="type"
        attrType="EString"/>
    <attributes
        attrName="numberOfSeats"
        attrType="EInt"/>
    <attributes
        attrName="id"
        attrType="EInt"/>
  </classes>
  <classes name="Luggage">
    <attributes
        attrName="number"
        attrType="EInt"/>
    <attributes
        attrName="ownerName"
        attrType="EString"/>
    <attributes
        attrName="ownerSurname"
        attrType="EString"/>
  </classes>
  <classes name="Terminal">
    <attributes
        attrName="id"
        attrType="EInt"/>
    <attributes
        attrName="capacity"
        attrType="EInt"/>
    <attributes
        attrName="open"
        attrType="EBoolean"/>
  </classes>
  <classes name="Tower">
    <attributes
        attrName="id"
        attrType="EInt"/>
    <attributes
        attrName="open"
        attrType="EBoolean"/>
    <attributes
        attrName="type"
        attrType="EString"/>
  </classes>
  <classes name="Observer">
    <attributes
        attrName="name"
        attrType="EString"/>
  </classes>
  <classes name="DispatcherOb"
      superClasses="//@classes.10">
    <attributes
        attrName="counter"
        attrType="EInt"/>
    <attributes
        attrName="waitingTSum"
        attrType="EInt"/>
    <attributes
        attrName="waitingTAv"
        attrType="EDouble"/>
    <attributes
        attrName="leghtSum"
        attrType="EInt"/>
    <attributes
        attrName="leghtAv"
        attrType="EDouble"/>
  </classes>
  <classes name="DeskOb"
      superClasses="//@classes.10">
    <attributes
        attrName="counter"
        attrType="EInt"/>
    <attributes
        attrName="openingTime"
        attrType="EInt"/>
    <attributes
        attrName="totalWorkingTime"
        attrType="EInt"/>
    <attributes
        attrName="closingTime"
        attrType="EInt"/>
    <attributes
        attrName="opencloseEvents"
        attrType="EInt"/>
    <attributes
        attrName="leghtSum"
        attrType="EInt"/>
    <attributes
        attrName="leghtAv"
        attrType="EDouble"/>
    <attributes
        attrName="waitingTSum"
        attrType="EInt"/>
    <attributes
        attrName="waitingTAv"
        attrType="EDouble"/>
  </classes>
  <classes name="GeneralOb"
      superClasses="//@classes.10">
    <attributes
        attrName="delayTAv"
        attrType="EDouble"/>
    <attributes
        attrName="delayTSum"
        attrType="EInt"/>
    <attributes
        attrName="counter"
        attrType="EInt"/>
    <attributes
        attrName="arrivalCounter"
        attrType="EInt"/>
    <attributes
        attrName="totalCost"
        attrType="EDouble"/>
    <attributes
        attrName="MaxWaitingTime"
        attrType="EInt"/>
    <attributes
        attrName="TotalTime"
        attrType="EInt"/>
  </classes>
  <classes name="SecurityClass"/>
  <classes name="Gate">
    <attributes
        attrName="number"
        attrType="EInt"/>
    <attributes
        attrName="open"
        attrType="EBoolean"/>
    <attributes
        attrName="capacity"
        attrType="EInt"/>
    <attributes
        attrName="terminal"
        attrType="EInt"/>
  </classes>
  <classes name="Taxiway"/>
  <classes name="Hangar"/>
</oopl:Program>
