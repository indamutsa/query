<?xml version="1.0" encoding="UTF-8"?>
<oopl:Program
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:oopl="http://SimpleOOPL/1.0"
    xsi:schemaLocation="http://SimpleOOPL/1.0 SimpleOOP.ecore"
    pName="College">
  <classes name="College">
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="address"
        attrType="EString"/>
    <attributes
        attrName="phoneNumber"
        attrType="EInt"/>
    <attributes
        attrName="email"
        attrType="EString"/>
    <attributes
        attrName="noActiveStudents"
        attrType="EInt"/>
    <attributes
        attrName="noEmployees"
        attrType="EInt"/>
  </classes>
  <classes name="Person"
      isAbstract="true">
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="surname"
        attrType="EString"/>
    <attributes
        attrName="address"
        attrType="EString"/>
    <attributes
        attrName="phoneNumber"
        attrType="EInt"/>
    <attributes
        attrName="email"
        attrType="EString"/>
    <attributes
        attrName="dayOfBirth"
        attrType="EDate"/>
  </classes>
  <classes name="Student"
      superClasses="//@classes.1">
    <attributes
        attrName="studentNumber"
        attrType="EInt"/>
    <attributes
        attrName="averageMark"
        attrType="EString"/>
    <attributes
        attrName="fullTime"
        attrType="EBoolean"/>
    <methods
        name="newStudent"
        returnType="EInt"/>
  </classes>
  <classes name="Professor"
      superClasses="//@classes.1">
    <attributes
        attrName="id"
        attrType="EInt"/>
    <attributes
        attrName="deparmet"
        attrType="EString"/>
    <attributes
        attrName="startDay"
        attrType="EInt"/>
    <methods
        name="newProfessor"
        returnType="EInt"/>
  </classes>
  <classes name="Enrollment">
    <attributes
        attrName="markRecieved"
        attrType="EString"/>
    <attributes
        attrName="day"
        attrType="EInt"/>
  </classes>
  <classes name="Seminar">
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="seminarId"
        attrType="EInt"/>
    <attributes
        attrName="fees"
        attrType="EDouble"/>
    <attributes
        attrName="classRoom"
        attrType="EString"/>
    <attributes
        attrName="dateAndHour"
        attrType="EDate"/>
    <attributes
        attrName="duration"
        attrType="EInt"/>
    <methods
        name="createSeminar"
        returnType="EInt"/>
    <methods
        name="modifySeminar"
        returnType="EInt"/>
    <methods
        name="eliminateSeminar"
        returnType="EInt"/>
  </classes>
  <classes name="Employee"
      superClasses="//@classes.1">
    <attributes
        attrName="id"
        attrType="EInt"/>
    <attributes
        attrName="deparmet"
        attrType="EString"/>
    <attributes
        attrName="startDay"
        attrType="EInt"/>
    <methods
        name="newEmployee"
        returnType="EInt"/>
  </classes>
  <classes name="Subject">
    <attributes
        attrName="id"
        attrType="EInt"/>
    <attributes
        attrName="credits"
        attrType="EInt"/>
    <attributes
        attrName="professor"
        attrType="EString"/>
    <attributes
        attrName="classRoom"
        attrType="EString"/>
    <attributes
        attrName="dateAndHour"
        attrType="EDate"/>
    <attributes
        attrName="fees"
        attrType="EDouble"/>
    <attributes
        attrName="duration"
        attrType="EInt"/>
    <methods
        name="createSubject"
        returnType="EInt"/>
    <methods
        name="modifySubject"
        returnType="EInt"/>
    <methods
        name="eliminateSubject"
        returnType="EInt"/>
  </classes>
</oopl:Program>
