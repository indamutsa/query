<?xml version="1.0" encoding="UTF-8"?>
<oopl:Program
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:oopl="http://SimpleOOPL/1.0"
    xsi:schemaLocation="http://SimpleOOPL/1.0 SimpleOOP.ecore"
    pName="MovieIMDB">
  <classes name="database">
    <attributes
        attrName="name"
        attrType="EString"/>
    <methods
        name="add_actor"
        returnType="EInt"/>
    <methods
        name="delete_actor"
        returnType="EInt"/>
    <methods
        name="modify_actor"
        returnType="EInt"/>
  </classes>
  <classes name="Genre_types">
    <attributes
        attrName="movie_id"
        attrType="EInt"/>
    <attributes
        attrName="movie_genre_type"
        attrType="EString"/>
  </classes>
  <classes name="Movie">
    <attributes
        attrName="id"
        attrType="EInt"
        isPublic="true"/>
    <attributes
        attrName="title"
        attrType="EString"
        isPublic="true"/>
    <attributes
        attrName="start_rating"
        attrType="EInt"/>
    <attributes
        attrName="running_time_minutes"
        attrType="EInt"
        isPublic="true"/>
    <attributes
        attrName="awards"
        attrType="Eint"/>
    <attributes
        attrName="admissions"
        attrType="EInt"/>
    <attributes
        attrName="trailer"
        attrType="EBoolean"/>
  </classes>
  <classes name="Actor">
    <attributes
        attrName="id"
        attrType="EInt"
        isPublic="true"/>
    <attributes
        attrName="name"
        attrType="EString"
        isPublic="false"/>
    <attributes
        attrName="address"
        attrType="EString"
        isPublic="false"/>
    <attributes
        attrName="rating"
        attrType="EInt"/>
    <attributes
        attrName="country"
        attrType="EString"/>
  </classes>
  <classes name="Movie_reviews">
    <attributes
        attrName="review_id"
        attrType="EInt"
        isPublic="true"/>
    <attributes
        attrName="review_date"
        attrType="EDate"/>
  </classes>
</oopl:Program>
