<?xml version="1.0" encoding="UTF-8"?>
<oopl:Program
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:oopl="http://SimpleOOPL/1.0"
    xsi:schemaLocation="http://SimpleOOPL/1.0 SimpleOOP.ecore"
    pName="SuchiRestaurant">
  <classes name="RestaurantSystem">
    <attributes
        attrName="name"
        attrType="Estring"/>
    <attributes
        attrName="profit"
        attrType="EDouble"/>
    <methods
        name="reserveSeat"
        returnType="EInt"/>
  </classes>
  <classes name="ConveyorBelt">
    <attributes
        attrName="capacity"
        attrType="EInt"/>
    <attributes
        attrName="freeSpots"
        attrType="Eint"/>
    <methods
        name="removeSushi"
        returnType="EInt"/>
  </classes>
  <classes name="Cooker">
    <attributes
        attrName="id"
        attrType="Eint"/>
    <methods
        name="newChef"
        returnType="EInt"/>
  </classes>
  <classes name="Sushi">
    <attributes
        attrName="id"
        attrType="Eint"/>
    <attributes
        attrName="price"
        attrType="EDouble"/>
  </classes>
  <classes name="Customer">
    <attributes
        attrName="id"
        attrType="EInt"/>
    <attributes
        attrName="name"
        attrType="Estring"/>
    <attributes
        attrName="type"
        attrType="EBoolean"/>
  </classes>
</oopl:Program>
