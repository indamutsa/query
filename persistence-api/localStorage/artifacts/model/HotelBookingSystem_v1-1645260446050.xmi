<?xml version="1.0" encoding="UTF-8"?>
<oopl:Program
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:oopl="http://SimpleOOPL/1.0"
    xsi:schemaLocation="http://SimpleOOPL/1.0 SimpleOOP.ecore"
    pName="HotelBookingSystem">
  <classes name="Hotel">
    <attributes
        attrName="street"
        attrType="EString"/>
    <attributes
        attrName="city"
        attrType="EString"/>
    <attributes
        attrName="zip"
        attrType="EString"/>
    <attributes
        attrName="coordinates"
        attrType="EString"/>
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="phoneNumber"
        attrType="EInt"/>
    <attributes
        attrName="website"
        attrType="EString"/>
    <methods
        name="newRoom"
        returnType="EInt"/>
    <methods
        name="newService"
        returnType="EInt"/>
    <methods
        name="deleteRoom"
        returnType="EInt"/>
    <methods
        name="deleteService"
        returnType="EInt"/>
    <methods
        name="modifyRoom"
        returnType="EInt"/>
    <methods
        name="modifyService"
        returnType="EInt"/>
  </classes>
  <classes name="Service">
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="description"
        attrType="EString"/>
    <attributes
        attrName="basePrice"
        attrType="EFloat"/>
  </classes>
  <classes name="Room">
    <attributes
        attrName="floor"
        attrType="EInt"/>
    <attributes
        attrName="door"
        attrType="EInt"/>
    <attributes
        attrName="capacity"
        attrType="EInt"/>
    <attributes
        attrName="price"
        attrType="EFloat"/>
  </classes>
  <classes name="Customer">
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="surname"
        attrType="EString"/>
    <attributes
        attrName="ident"
        attrType="EString"/>
    <attributes
        attrName="email"
        attrType="EString"/>
    <attributes
        attrName="phoneNumeber"
        attrType="EInt"/>
    <methods
        name="newCustomer"
        returnType="EInt"/>
    <methods
        name="modifyCustomer"
        returnType="EInt"/>
    <methods
        name="deleteCustomer"
        returnType="EInt"/>
  </classes>
</oopl:Program>
