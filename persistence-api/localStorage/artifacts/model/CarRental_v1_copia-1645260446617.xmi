<?xml version="1.0" encoding="UTF-8"?>
<oopl:Program
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:oopl="http://SimpleOOPL/1.0"
    xsi:schemaLocation="http://SimpleOOPL/1.0 SimpleOOP.ecore"
    pName="CarRental">
  <classes name="Client">
    <attributes
        attrName="user_id"
        attrType="EInt"/>
    <attributes
        attrName="user_name"
        attrType="EString"/>
    <attributes
        attrName="user_add"
        attrType="EString"/>
    <attributes
        attrName="user_email"
        attrType="EString"/>
    <attributes
        attrName="mobile"
        attrType="EDouble"/>
    <attributes
        attrName="dob"
        attrType="EDate"/>
  </classes>
  <classes name="Vehicle">
    <attributes
        attrName="vehicle_type"
        attrType="EString"/>
    <attributes
        attrName="vehicle_id"
        attrType="EInt"/>
    <attributes
        attrName="manufacter"
        attrType="EString"/>
    <attributes
        attrName="power"
        attrType="EString"/>
    <attributes
        attrName="reg_number"
        attrType="EInt"/>
  </classes>
  <classes name="Car"
      superClasses="//@classes.1">
    <attributes
        attrName="capacity"
        attrType="EInt"/>
  </classes>
  <classes name="Payment">
    <attributes
        attrName="payment_id"
        attrType="EInt"/>
    <attributes
        attrName="payment_type"
        attrType="EString"/>
    <attributes
        attrName="payment_details"
        attrType="EString"/>
    <attributes
        attrName="rewards"
        attrType="EString"/>
    <methods
        name="make_payment"
        returnType="EInt"/>
    <methods
        name="register_credit_card"
        returnType="EInt"/>
  </classes>
  <classes name="Driver">
    <attributes
        attrName="driver_id"
        attrType="EInt"/>
    <attributes
        attrName="driver_name"
        attrType="EString"/>
    <attributes
        attrName="driver_add"
        attrType="EString"/>
    <attributes
        attrName="driver_email"
        attrType="EString"/>
    <attributes
        attrName="driver_mobile"
        attrType="EDouble"/>
    <attributes
        attrName="birthday"
        attrType="EDate"/>
    <methods
        name="sign_up"
        returnType="EInt"/>
    <methods
        name="login"
        returnType="EBoolean"/>
    <methods
        name="logout"
        returnType="EBoolean"/>
  </classes>
  <classes name="Reservation">
    <attributes
        attrName="start_day"
        attrType="EDate"/>
    <attributes
        attrName="end_day"
        attrType="EDate"/>
    <attributes
        attrName="user_id"
        attrType="EInt"/>
  </classes>
</oopl:Program>
