<?xml version="1.0" encoding="UTF-8"?>
<oopl:Program
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:oopl="http://SimpleOOPL/1.0"
    xsi:schemaLocation="http://SimpleOOPL/1.0 SimpleOOP.ecore"
    pName="OnlinePay">
  <classes name="Customer">
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="deliveryAddress"
        attrType="EString"/>
    <attributes
        attrName="contact"
        attrType="EString"/>
    <attributes
        attrName="active"
        attrType="EBoolean"/>
  </classes>
  <classes name="Order">
    <attributes
        attrName="createDate"
        attrType="EDate"/>
  </classes>
  <classes name="Payment">
    <attributes
        attrName="amount"
        attrType="EFloat"/>
  </classes>
  <classes name="Item">
    <attributes
        attrName="weight"
        attrType="EFloat"/>
    <attributes
        attrName="description"
        attrType="EString"/>
  </classes>
  <classes name="OrderDetail">
    <attributes
        attrName="qty"
        attrType="EInt"/>
    <attributes
        attrName="taxStatus"
        attrType="EString"/>
  </classes>
  <classes name="Credit">
    <attributes
        attrName="number"
        attrType="EString"/>
    <attributes
        attrName="type"
        attrType="EString"/>
    <attributes
        attrName="expireDate"
        attrType="EDate"/>
  </classes>
  <classes name="Cash">
    <attributes
        attrName="cashTendered"
        attrType="EFloat"/>
  </classes>
  <classes name="Check">
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="bankID"
        attrType="EString"/>
  </classes>
  <classes name="WireTransfer">
    <attributes
        attrName="bankID"
        attrType="EString"/>
    <attributes
        attrName="bankName"
        attrType="EString"/>
  </classes>
</oopl:Program>
