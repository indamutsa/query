<?xml version="1.0" encoding="UTF-8"?>
<oopl:Program
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:oopl="http://SimpleOOPL/1.0"
    xsi:schemaLocation="http://SimpleOOPL/1.0 SimpleOOP.ecore"
    pName="Airport">
  <classes name="Airport">
    <attributes
        attrName="businessName"
        attrType="EString"/>
    <attributes
        attrName="businessCountry"
        attrType="EString"/>
    <attributes
        attrName="checkInDesk"
        attrType="EBoolean"/>
    <methods
        name="openCheckInDesk"
        returnType="EBoolean"/>
    <methods
        name="terminateCheckInDesk"
        returnType="EBoolean"/>
  </classes>
  <classes name="Human">
    <attributes
        attrName="id"
        attrType="EInt"/>
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="lastname"
        attrType="EString"/>
    <attributes
        attrName="email"
        attrType="EString"/>
    <attributes
        attrName="mobile"
        attrType="EInt"/>
    <attributes
        attrName="day_of_birthday"
        attrType="EDate"/>
  </classes>
  <classes name="Driver"
      superClasses="//@classes.1">
    <attributes
        attrName="classification"
        attrType="EString"/>
  </classes>
  <classes name="Traveller"
      superClasses="//@classes.1">
    <attributes
        attrName="regularTraveller"
        attrType="EBoolean"/>
    <attributes
        attrName="hasTicket"
        attrType="EBoolean"/>
  </classes>
  <classes name="Flight">
    <attributes
        attrName="flight_number"
        attrType="EString"/>
    <attributes
        attrName="flight_date"
        attrType="EDate"/>
    <attributes
        attrName="boarding_hour_day"
        attrType="EDate"/>
    <attributes
        attrName="gate"
        attrType="EString"/>
    <attributes
        attrName="flight_from"
        attrType="EString"/>
    <attributes
        attrName="flight_to"
        attrType="EString"/>
    <attributes
        attrName="flight_regulation"
        attrType="EString"/>
    <attributes
        attrName="state"
        attrType="EString"/>
  </classes>
  <classes name="Ticket">
    <attributes
        attrName="no"
        attrType="EInt"/>
    <attributes
        attrName="flight_number"
        attrType="EString"/>
    <attributes
        attrName="flight_date"
        attrType="EDate"/>
    <attributes
        attrName="boarding_hour_time"
        attrType="EDate"/>
    <attributes
        attrName="gate"
        attrType="EString"/>
    <attributes
        attrName="flight_from"
        attrType="EString"/>
    <attributes
        attrName="flight_to"
        attrType="EString"/>
  </classes>
  <classes name="Plane">
    <attributes
        attrName="categorization"
        attrType="EString"/>
    <attributes
        attrName="seats_amount"
        attrType="EInt"/>
    <attributes
        attrName="id"
        attrType="EInt"/>
  </classes>
  <classes name="Gear">
    <attributes
        attrName="no"
        attrType="EInt"/>
    <attributes
        attrName="name_owner"
        attrType="EString"/>
    <attributes
        attrName="lastname_owner"
        attrType="EString"/>
  </classes>
  <classes name="Terminal">
    <attributes
        attrName="id"
        attrType="EInt"/>
    <attributes
        attrName="room_capacity"
        attrType="EInt"/>
    <attributes
        attrName="working"
        attrType="EBoolean"/>
  </classes>
  <classes name="Tower">
    <attributes
        attrName="id"
        attrType="EInt"/>
    <attributes
        attrName="working"
        attrType="EBoolean"/>
    <attributes
        attrName="classification"
        attrType="EString"/>
  </classes>
  <classes name="Gate">
    <attributes
        attrName="no"
        attrType="EInt"/>
    <attributes
        attrName="working"
        attrType="EBoolean"/>
    <attributes
        attrName="capacity"
        attrType="EInt"/>
    <attributes
        attrName="terminal"
        attrType="EInt"/>
  </classes>
</oopl:Program>
