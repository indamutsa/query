<?xml version="1.0" encoding="UTF-8"?>
<oopl:Program
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:oopl="http://SimpleOOPL/1.0"
    xsi:schemaLocation="http://SimpleOOPL/1.0 SimpleOOP.ecore"
    pName="Library">
  <classes name="Author">
    <attributes
        attrName="id"
        attrType="EInt"/>
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="surname"
        attrType="EString"/>
    <attributes
        attrName="nacionality"
        attrType="EString"/>
    <methods
        name="addAuthor"
        returnType="EInt"/>
  </classes>
  <classes name="Category">
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="id"
        attrType="EInt"/>
    <methods
        name="addCategory"
        returnType="EString"/>
  </classes>
  <classes name="Book">
    <attributes
        attrName="id"
        attrType="EInt"/>
    <attributes
        attrName="title"
        attrType="EString"/>
    <attributes
        attrName="copies"
        attrType="EInt"/>
    <attributes
        attrName="edition"
        attrType="EInt"/>
    <attributes
        attrName="genere"
        attrType="EString"/>
    <attributes
        attrName="pages"
        attrType="EInt"/>
    <methods
        name="putOnShelf"
        returnType="EBoolean"/>
    <methods
        name="lost"
        returnType="EInt"/>
    <methods
        name="find"
        returnType="EInt"/>
    <methods
        name="check"
        returnType="EBoolean"/>
  </classes>
  <classes name="Library">
    <attributes
        attrName="name"
        attrType="EString"/>
    <attributes
        attrName="address"
        attrType="EString"/>
    <attributes
        attrName="phoneNumber"
        attrType="EString"/>
    <methods
        name="check"
        returnType="EInt"/>
    <methods
        name="checkout"
        returnType="EInt"/>
    <methods
        name="return"
        returnType="EBoolean"/>
  </classes>
  <classes name="Member">
    <attributes
        attrName="membershipType"
        attrType="EString"/>
    <attributes
        attrName="name"
        attrType="EString"/>
    <methods
        name="enroll"
        returnType="EInt"/>
    <methods
        name="payFine"
        returnType="EInt"/>
  </classes>
  <classes name="Loan">
    <attributes
        attrName="date"
        attrType="EDate"/>
    <attributes
        attrName="bookId"
        attrType="EInt"/>
    <attributes
        attrName="from"
        attrType="EDate"/>
    <attributes
        attrName="to"
        attrType="EDate"/>
    <attributes
        attrName="status"
        attrType="EString"/>
    <attributes
        attrName="comments"
        attrType="EString"/>
    <methods
        name="check"
        returnType="EInt"/>
    <methods
        name="checkout"
        returnType="EInt"/>
    <methods
        name="return"
        returnType="EBoolean"/>
  </classes>
</oopl:Program>
